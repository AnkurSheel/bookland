{
    "name": "bookland",
    "private": true,
    "description": "A blog for book reviews",
    "version": "0.1.0",
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
        "start": "npm run develop",
        "serve": "gatsby serve",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
        "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
        "lint:fix": "lint --fix",
        "generate-graphql": "graphql-codegen --config codegen.yml",
        "deduplicate": "yarn-deduplicate"
    },
    "dependencies": {
        "@codinators/gatsby-shared-library": "^1.1.3",
        "gatsby": "^2.13.31",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-helmet": "^5.2.1"
    },
    "devDependencies": {
        "@emotion/core": "^10.0.14",
        "@emotion/styled": "^10.0.14",
        "@graphql-codegen/cli": "^1.4.0",
        "@graphql-codegen/typescript": "^1.4.0",
        "@graphql-codegen/typescript-operations": "^1.4.0",
        "@mdx-js/mdx": "^1.1.0",
        "@mdx-js/react": "^1.0.27",
        "@types/react": "^16.8.24",
        "@types/react-helmet": "^5.0.8",
        "@typescript-eslint/eslint-plugin": "^1.13.0",
        "@typescript-eslint/parser": "^1.13.0",
        "eslint": "^6.1.0",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.14.2",
        "gatsby-background-image": "^0.9.12",
        "gatsby-image": "^2.2.6",
        "gatsby-plugin-emotion": "^4.1.2",
        "gatsby-plugin-mdx": "^1.0.15",
        "gatsby-plugin-react-helmet": "^3.1.2",
        "gatsby-plugin-sharp": "^2.2.9",
        "gatsby-plugin-typescript": "^2.1.2",
        "gatsby-remark-images": "^3.1.7",
        "gatsby-source-filesystem": "^2.1.6",
        "gatsby-transformer-sharp": "^2.2.4",
        "prettier": "^1.18.2",
        "typescript": "^3.5.3",
        "yalc": "^1.0.0-pre.34",
        "yarn-deduplicate": "^1.1.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
    },
    "bugs": {
        "url": "https://github.com/gatsbyjs/gatsby/issues"
    }
}
